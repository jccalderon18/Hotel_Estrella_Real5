    <script>
        let index = 0;
        const Imagenes = document.querySelectorAll('.fade');

        function showNextImage() {
            Imagenes[index].classList.remove('show');
            index = (index + 1) % Imagenes.length;
            Imagenes[index].classList.add('show');
        }

        Imagenes[index].classList.add('show'); // Mostrar la primera imagen
        setInterval(showNextImage, 3000); // Cambiar cada 3 segundos

    </script>
    <script>
        function openModal(element) {
            const modal = document.getElementById("modal");
            const modalImage = document.getElementById("modal-image");
            const modalText = document.getElementById("modal-text"); // Nuevo elemento para mostrar el texto

            // Mostrar el modal
            modal.style.display = "flex";

            // Obtener y asignar la imagen al modal
            modalImage.src = element.querySelector("img").src;

            // Obtener y asignar el texto desde el atributo 'data-text' al modal
            const text = element.querySelector("img").getAttribute("data-text");
            modalText.innerText = text; // Mostrar el texto en el modal
        }

        function closeModal() {
            const modal = document.getElementById("modal");
            modal.style.display = "none";
        }

        // Script para el modal de servicios
        function showModal(element) {
            const modal = document.getElementById("imageModal");
            const modalImg = document.getElementById("modalImg");
            modal.style.display = "flex";
            modalImg.src = element.querySelector("img").src;
        }

        function hideModal() {
            const modal = document.getElementById("imageModal");
            modal.style.display = "none";
        }
    </script>
